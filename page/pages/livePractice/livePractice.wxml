<view class="live-practice">
    <!-- 在线陪练情况 -->
    <image class="head-title" src="{{headImage}}" mode="aspectFit" />
    <image class="info" src="{{infoImage}}" mode="aspectFit" />
    <view class="parent-view">
        <image class="parent-image" src="{{parentImage}}" mode="aspectFit" bindtap="videoShow" wx:if="{{!videoStatus}}" />
        <video src="{{parentVideo}}" autoplay="true" controls wx:if="{{videoStatus}}" ></video>
    </view>
    <image class="intro" src="{{introImage}}" mode="aspectFit" />
</view>
