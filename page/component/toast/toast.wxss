
.toast_content_box{
	display: flex;
	width: 100%;
	height: 100%;
	justify-content:center;
	align-items: center;
	position: fixed;
	z-index: 999;
}
.toast_content{
	width: 50%;
	padding: 10rpx;
	background: rgba(0, 0, 0, 0.8);
	border-radius: 20rpx;
}
.toast_content_text{
	width: 100%;
	height: 100%;
	color: #fff;
	font-size: 20rpx;
	text-align: center;
}
